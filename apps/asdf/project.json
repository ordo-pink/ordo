{
  "name": "asdf",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/asdf/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/asdf",
        "baseHref": "/",
        "main": "apps/asdf/src/main.tsx",
        "tsConfig": "apps/asdf/tsconfig.app.json",
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/asdf/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/asdf/src/environments/environment.ts",
              "with": "apps/asdf/src/environments/environment.prod.ts"
            }
          ],
          "optimization": false,
          "outputHashing": "none",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "asdf:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "asdf:build:development"
        },
        "production": {
          "buildTarget": "asdf:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/asdf/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/asdf/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}

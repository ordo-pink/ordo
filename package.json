{
  "name": "@ordo-pink/backend-universal",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.0",
  "description": "Ordo.pink backend server for personal use.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "serve": "node dist/index.js",
    "start": "concurrently \"webpack --watch\" \"nodemon -q ./dist\"",
    "test": "jest",
    "test:cover": "jest --coverage",
    "test:watch": "jest --watch",
    "tsc:check": "tsc --noEmit",
    "eslint:check": "eslint --ext .ts,.tsx .",
    "prettier:check": "prettier --check src/**/*.{ts,tsx}",
    "prettier:format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "npm run eslint:check && npm run prettier:check && npm run tsc:check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ordo-pink/ordo-backend-universal.git"
  },
  "keywords": [],
  "author": {
    "email": "hello@ordo.pink",
    "name": "Ordo.pink",
    "url": "https://ordo.pink"
  },
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/ordo-pink/ordo-backend-universal/issues"
  },
  "homepage": "https://github.com/ordo-pink/ordo-backend-universal#readme",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.18",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "typescript-declaration-webpack-plugin": "^0.2.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@ordo-pink/switch": "^1.1.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",

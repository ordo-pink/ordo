{
  "name": "extension-editor",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/extension-editor/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/extension-editor",
        "main": "packages/extension-editor/src/index.ts",
        "tsConfig": "packages/extension-editor/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/extension-editor/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/extension-editor/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
